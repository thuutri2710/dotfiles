// Place your key bindings in this file to overwrite the defaults
[
	{
		"key": "ctrl+shift+n",
		"command": "explorer.newFile"
	},
	{
		"key": "cmd+k ctrl+shift+n",
		"command": "explorer.newFolder"
	},
	{
		"key": "f",
		"command": "explorer.newFile",
		"when": "explorerViewletVisible && explorerViewletFocus && !inputFocus"
	},
	{
		"key": "shift+f",
		"command": "explorer.newFolder",
		"when": "explorerViewletVisible && explorerViewletFocus && !inputFocus"
	},
	{
		"key": "d",
		"command": "deleteFile",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
	},
	{
		"key": "cmd+r",
		"command": "workbench.action.openRecent"
	},
	{
		"key": "ctrl+r",
		"command": "-workbench.action.openRecent"
	},
	{
		"key": "cmd+k 1",
		"command": "workbench.action.terminal.focusAtIndex1"
	},
	{
		"key": "cmd+k 2",
		"command": "workbench.action.terminal.focusAtIndex2"
	},
	{
		"key": "cmd+k 3",
		"command": "workbench.action.terminal.focusAtIndex3"
	},
	{
		"key": "cmd+k cmd+'",
		"command": "workbench.action.terminal.newInActiveWorkspace"
	},
	{
		"key": "cmd+;",
		"command": "terminal.toggle"
	},
	{
		"key": "cmd+'",
		"command": "workbench.action.terminal.focus"
	},
	{
		"key": "cmd+backspace",
		"command": "workbench.action.terminal.kill",
		"when": "terminalFocus"
	},
	{
		"key": "cmd+shift+down",
		"command": "search.action.focusNextSearchResult",
		"when": "hasSearchResult && searchViewletVisible"
	},
	{
		"key": "cmd+shift+up",
		"command": "search.action.focusPreviousSearchResult",
		"when": "hasSearchResult && searchViewletVisible"
	},
	{
		"key": "ctrl+cmd+alt+w",
		"command": "cursorWordEndRight",
		"when": "textInputFocus"
	},
	{
		"key": "ctrl+cmd+alt+b",
		"command": "cursorWordEndLeft",
		"when": "textInputFocus"
	},
	{
		"key": "cmd+[",
		"command": "workbench.action.navigateBack",
		"when": ""
	},
	{
		"key": "cmd+]",
		"command": "workbench.action.navigateForward",
		"when": ""
	},
	{
		"key": "ctrl+cmd+alt+3",
		"command": "editor.action.goToDeclaration",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+cmd+alt+2",
		"command": "editor.emmet.action.balanceOut"
	},
	{
		"key": "ctrl+alt+cmd+5",
		"command": "editor.emmet.action.matchTag"
	},
	{
		"key": "ctrl+cmd+alt+shift+,",
		"command": "bookmarks.listFromAllFiles"
	},
	{
		"key": "ctrl+cmd+alt+m",
		"command": "bookmarks.toggle",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+cmd+alt+a m",
		"command": "bookmarks.toggleLabeled",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+cmd+alt+f",
		"command": "editor.action.previewDeclaration",
		"when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+shift+cmd+alt 3",
		"command": "editor.action.openDeclarationToTheSide",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+cmd+alt+shift+'",
		"command": "extension.selectEitherQuote",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+]",
		"command": "extension.selectSquareBrackets",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+shift+]",
		"command": "extension.selectCurlyBrackets",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+0",
		"command": "extension.selectParenthesis",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+shift+a",
		"command": "extension.selectBackTick",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+`",
		"command": "extension.selectBackTick",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+'",
		"command": "extension.selectSingleQuote",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+cmd+alt 0",
		"command": "extension.selectParenthesis",
		"when": "editorTextFocus"
	},
	{
		"key": "shift+cmd+alt+0",
		"command": "extension.selectParenthesis",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+alt+shift+space",
		"command": "editor.action.deleteLines",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+cmd+alt+shift+2",
		"command": "extension.selectInTag"
	},
	{
		"key": "ctrl+shift+alt+shift+k",
		"command": "extension.selectInTag"
	},
	{
		"key": "ctrl+alt+cmd+g r",
		"command": "gitlens.openFileRevision"
	},
	{
		"key": "ctrl+alt+cmd+g h",
		"command": "gitlens.showFileHistoryInView"
	},
	{
		"key": "ctrl+alt+cmd+g b",
		"command": "gitlens.toggleLineBlame"
	},
	{
		"key": "ctrl+alt+cmd+g c",
		"command": "git.openChange"
	},
	{
		"key": "ctrl+alt+cmd+g z",
		"command": "gitlens.toggleZenMode"
	},
	{
		"key": "ctrl+alt+cmd+g m",
		"command": "gitlens.toggleFileHeatmap"
	},
	{
		"key": "ctrl+alt+cmd+g f",
		"command": "gitlens.toggleFileRecentChanges"
	},
	{
		"key": "shift+alt+w",
		"command": "editor.emmet.action.wrapIndividualLinesWithAbbreviation"
	},
	{
		"key": "ctrl+alt+cmd+space",
		"command": "editor.action.triggerSuggest",
		"when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
	},
	{
		"key": "ctrl+alt+cmd+4",
		"command": "editor.action.peekDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+shift+alt+cmd+4",
		"command": "editor.action.referenceSearch.trigger",
		"when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+shift+alt+cmd+left",
		"command": "editor.action.smartSelect.shrink",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+cmd+right",
		"command": "editor.action.smartSelect.expand",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+cmd+up",
		"command": "cursorMove",
		"args": {
			"to": "up",
			"by": "line",
			"value": 10
		},
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+cmd+down",
		"command": "cursorMove",
		"args": {
			"to": "down",
			"by": "line",
			"value": 10
		},
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+cmd+\\",
		"command": "workbench.files.action.showActiveFileInExplorer"
	},
	{
		"key": "ctrl+shift+alt+cmd+r",
		"command": "revealInExplorer"
	},
	{
		"key": "ctrl+shift+alt+cmd+y",
		"command": "workbench.explorer.fileView.focus"
	},
	{
		"key": "ctrl+alt+cmd+u",
		"command": "undo",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "ctrl+alt+cmd+r",
		"command": "redo",
		"when": "textInputFocus && !editorReadonly"
	},
