{
	"workbench.statusBar.visible": true,
	//"editor.minimap.enabled": false,
	"editor.fontFamily": "FiraCode-Retina",
	"editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
	"breadcrumbs.enabled": true,
	"editor.suggestSelection": "first",
	"mssql.intelliSense.lowerCaseSuggestions": true,
	"python.jediEnabled": false,
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.triggerExpansionOnTab": true,
	"files.associations": {
		"*.html": "html"
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.fontLigatures": true,
	"window.menuBarVisibility": "default",
	"editor.minimap.enabled": true,
	"workbench.colorTheme": "One Monokai Dark",
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"sync.gist": "c7bfa003f8d9f510c2655ced75dc2197",
	"python.languageServer": "Pylance",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true,
	"javascript.validate.enable": false,
	"eslint.run": "onType",
	"terminal.integrated.fontFamily": "RobotoMono Nerd Font",
	"terminal.integrated.cursorStyle": "line",
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.shell.osx": "/bin/zsh",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.editor.enablePreview": false,
	"editor.largeFileOptimizations": false,
	"codespaces.accountProvider": "Uninitialized",
	"explorer.confirmDelete": false,
	"cSpell.language": "en-GB,en",
	"typescript.tsdk": "~/node_modules/typescript/lib",
	"typescript.validate.enable": false,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"cSpell.enableFiletypes": ["jupyter"],
	"jupyter.alwaysTrustNotebooks": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.activityBar.visible": false,
	"[react]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"terminal.integrated.tabs.enabled": false,
	"security.workspace.trust.untrustedFiles": "open"
}
